{
  "name": "api",
  "version": "1.3.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "ts-node-dev --transpile-only --no-notify src/main/server.ts",
    "start": "node dist/main/server.js",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand --verbose",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test --findRelatedTests ./src",
    "test:coverage": "npm test --coverage",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migrate:generate": "yarn build && yarn typeorm migration:generate -n",
    "migrate:run": "yarn build && yarn typeorm migration:run",
    "migrate:drop": "yarn build && yarn typeorm migration:revert",
    "migrate:delete": "rm -r -f src/database/migrations/*",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "openapi-types": "^12.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
