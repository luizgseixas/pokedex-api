{
  "name": "api",
  "version": "1.4.3",
  "description": "",
  "main": "src/main/server.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "dev": "cross-env NODE_ENV=DEVELOP ts-node-dev --transpile-only --respawn --watch src src/main/server.ts",
    "start": "cross-env NODE_ENV=PRODUCTION node dist/main/server.js",
    "test": "jest --passWithNoTests --detectOpenHandles --no-cache --silent --verbose",
    "test:verbose": "jest --passWithNoTests --verbose --silent",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test --findRelatedTests ./src",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prisma:migration:generate": "prisma migrate dev --name",
    "prisma:migration:apply": "npx prisma migrate deploy",
    "migration:create": "typeorm migration:create",
    "migration:generate": "yarn build && yarn typeorm migration:generate -d ./dist/infra/typeorm/postgres-datasource.js",
    "migration:run": "yarn build && yarn typeorm migration:run -d ./dist/infra/typeorm/postgres-datasource.js",
    "migration:revert": "yarn build && yarn typeorm migration:revert -d ./dist/infra/typeorm/postgres-datasource.js",
    "migration:delete": "rm -r -f src/infra/typeorm/migrations/*",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "axios": "^1.2.1",
    "bull": "^4.10.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.17",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "openapi-types": "^12.1.0",
    "pg-mem": "^2.6.4",
    "prisma": "^4.13.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "*",
    "uuid": "^9.0.0"
  }
}
